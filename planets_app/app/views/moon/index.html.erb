<h1>Moons</h1>
<table>
    <% @moons.each do |moon| %>
        <tr>
            <td id ='moonName'><%= moon.name %></td>
            <td><%= moon.distance_from_planet %></td>
            <td><%= moon.radius%></td>
            <td><%= moon.orbit_period %></td>
            <td><%= moon.planet %></td>
        </tr>
    <% end %>
</table>